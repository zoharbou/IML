\begin_layout Standard
מתקיים 
\begin_inset Formula 
\[
f\left(\sigma\right)=U\cdot diag\left(\sigma\right)\cdot U^{T}x
\]

\end_inset


\begin_inset Formula 
\[
h\left(\sigma\right)=\frac{1}{2}\left|\left|f\left(\sigma\right)-y\right|\right|^{2}
\]

\end_inset


\begin_inset Newline newline
\end_inset

צריך לחשב את הגרדיאנט של 
\begin_inset Formula $h$
\end_inset

.
 נגדיר 
\begin_inset Formula $g\left(\bar{u}\right)=\frac{1}{2}\left|\left|\bar{u}-y\right|\right|^{2}$
\end_inset

 
\begin_inset Formula $g:\mathbb{R}^{n}\rightarrow\mathbb{R}$
\end_inset

, אזי 
\begin_inset Formula $h=g\circ f\left(\sigma\right)$
\end_inset

 ומכלל השרשרת 
\begin_inset Formula $J_{\sigma}\left(g\circ f\right)=J_{f\left(\sigma\right)}\left(g\right)\cdot J_{\sigma}\left(f\right)$
\end_inset


\begin_inset Newline newline
\end_inset

נשים לב כי את הגרדיאנט של 
\begin_inset Formula $g$
\end_inset

 אנו מכירים מהכיתה כלומר 
\begin_inset Formula $\Delta g\left(u\right)=2\cdot\frac{1}{2}\left(\bar{u}-y\right)^{T}=\left(\bar{u}-y\right)^{T}$
\end_inset

, לכן
\begin_inset Formula 
\[
J_{\sigma}\left(g\circ f\right)=J_{f\left(\sigma\right)}\left(g\right)\cdot J_{\sigma}\left(f\right)=\left(\underset{f\left(\sigma\right)}{\underbrace{U\cdot diag\left(\sigma\right)\cdot U^{T}x}}-y\right)^{T}\cdot\begin{bmatrix}\left\langle u_{1},x\right\rangle \left[u_{1}\right]_{1} &  & \cdots &  & \left\langle u_{n},x\right\rangle \left[u_{n}\right]_{1}\\
\\
 &  & \vdots\\
\\
\left\langle u_{1},x\right\rangle \left[u_{1}\right]_{n} &  & \cdots &  & \left\langle u_{n},x\right\rangle \left[u_{n}\right]_{n}
\end{bmatrix}
\]

\end_inset


\begin_inset Newline newline
\end_inset


\end_layout

\begin_layout Subsection*
